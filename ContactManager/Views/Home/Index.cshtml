@model List<Contact>@{ ViewBag.Title = "Contact Manager"; }<a asp-controller="Contact" asp-action="Add">Add Contact</a><table class="table table-bordered table-striped">    <thead>        <tr>            <th>First Name</th>            <th>Last Name</th>            <th>Phone</th>            <th>Email</th>            <th>Category</th>        </tr>    </thead>    <tbody>        @foreach (var contact in Model)        {            <tr>                <td><a asp-controller="Home" asp-action="Details" asp-route-id="@contact.ContactId">@contact.FirstName</a></td>                <td><a asp-controller="Home" asp-action="Details" asp-route-id="@contact.ContactId">@contact.LastName</a></td>                <td>@contact.PhoneNumber</td>                <td>@contact.Email</td>                <td>@contact.Category.Name</td>            </tr>         }    </tbody></table>